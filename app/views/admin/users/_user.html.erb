<tr>
	<td><%= user.id if user.id %></td>
	<td><%= user.name if user.name %> <%= user.surnames if user.surnames %></td>
	<td><%= user.place.name if user.place_id %></td>
	<td><%= user.email if user.email %></td>
	<td><%= user.phone if user.phone %></td>
	<td><%= link_to t('boton.mostrar'), admin_user_path(user), class: 'btn btn-default' %></td>
	<td><%= link_to t('boton.editar'), edit_admin_user_path(user), class: 'btn btn-default' %></td>
	<td><%= link_to t('boton.eliminar'), admin_user_path(user),
              method: :delete,
              data: { confirm: 'Are you sure?' }, class: 'btn btn-default' %></td>
</tr>