            </div>
            <div class="col-xs-6">
                <div class="card-single-direction text-uppercase text-center"><h4><%= @place.town %></h4></div>
                <img class="single-img img-responsive img-rounded" src="<%= @place.picture %>"></img>
            </div>
        </div>
