            <div class="col-xs-6">
                <h1 class="card-title text-uppercase"><%= @place.name %></h1>
                    <p class="single-description" id="descripcion"><%= @place.description %></p>
                    <p class="single-description" id="descripcion"><%= @place.time %></p>
